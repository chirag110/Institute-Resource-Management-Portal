@model IEnumerable<EmptyProject.Models.Institute>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Inst_Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inst_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inst_Address)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Inst_Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inst_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inst_Address)
            </td>

        </tr>
    }

</table>